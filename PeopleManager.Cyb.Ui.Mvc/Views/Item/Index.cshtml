@model IList<ShopItem>
@{
    ViewData["Title"] = "Items";
}

<h1>@ViewData["Title"]</h1>

<a class="btn btn-primary" asp-action="Create">New Item</a>

<table class="table table-striped table-dark">
    <thead>
    <tr>
        <th>Item Name</th>
        <th>Price</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
            <partial name="ItemPartial" model="item" />
    }
    </tbody>
</table>